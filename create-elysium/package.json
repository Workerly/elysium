{
	"name": "create-elysium",
	"version": "0.1.7",
	"license": "Apache-2.0",
	"description": "A super-powered web framework built on top of Bun and Elysia.",
	"author": {
		"name": "Axel Nana",
		"email": "axel.nana@workbud.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Workerly/elysium"
	},
	"homepage": "https://github.com/Workerly/elysium",
	"scripts": {
		"format": "prettier --write \"./**/*.ts\"",
		"build": "bun build index.ts --outfile=index.js --target bun --minify"
	},
	"keywords": [
		"elysium",
		"framework",
		"web",
		"api",
		"typescript"
	],
	"bin": {
		"create-elysium": "./index.js"
	},
	"files": [
		"index.js",
		"template-*/**",
		"feature-*/**",
		"shared-*/**",
		"plugin-*/**"
	],
	"type": "module",
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "^4.4.1",
		"@types/bun": "latest",
		"@types/prompts": "^2.4.9",
		"prettier": "^3.5.3"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"prompts": "^2.4.2",
		"radash": "^12.1.0"
	}
}
